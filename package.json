{
  "name": "covid-19-estimator-rest-api",
  "version": "1.0.0",
  "description": "A REST api for covid-19-estimator",
  "main": "./src/index.js",
  "directories": {
    "test": "./src/__tests__"
  },
  "scripts": {
    "build": "babel --out-dir dist --ignore __tests__",
    "dev": "babel-watch ./server.js",
    "start": "nodemon --exec babel-node ./server.js",
    "test": "cross-env NODE_ENV=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint": "eslint ./*.js",
    "heroku-postbuild": "npm run start"
  },
  "keywords": [
    "REST",
    "api",
    "covid-19-estimator"
  ],
  "dependencies": {
    "babel-watch": "^7.0.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "json2xml": "^0.1.3",
    "morgan": "^1.10.0",
    "prettier": "^2.0.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^25.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2"
  },
  "author": "Desire Kaleba",
  "license": "MIT",
  "engines": {
    "node": "12.7.0"
  }
}
